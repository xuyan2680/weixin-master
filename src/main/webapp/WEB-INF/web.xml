<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>weixinServlet</servlet-name>
        <servlet-class>com.chinagas.weixin.servlet.WeixinServlet</servlet-class>
    </servlet>
     <servlet>
        <servlet-name>weixinJumpServlet</servlet-name>
        <servlet-class>com.chinagas.weixin.servlet.WeixinJump</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>weixinServlet</servlet-name>
        <url-pattern>/wx.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>weixinJumpServlet</servlet-name>
        <url-pattern>/wxjump.do</url-pattern>
    </servlet-mapping>
</web-app>
